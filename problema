#include <iostream>
#include<bitset>

using namespace std;

class BitSet {
    char v[10];
public:
    BitSet (){
        for(int i=0; i<10; i++)
            v[i] = 0;
    }
    void add (int x){
        int index = x/8;
        char q = v[index] | (1 << (7-x%8));
        v[index] = q;
    }

    bool contains (int x){
        char q = 1 << (7-x%8);
        for (int i=0; i<10; i++)
            if(v[i] & q)
                return true;
        return false;
    }

    void elimina (int x){
        v[x/8] = v[x/8] & ~ (1 << (7-x%8));
    }

    void toString (){
        for(int i=0; i<9; i++)
        {
            if(contains(i))
                cout << i << " ";
        }
    }

};

int main()
{
    int val, dim;
    BitSet a;

    cout << "Cu cate elemente lucrati?: ";   cin >> dim;
    for(int i=0; i<dim; i++)
    {
        cout << "ntroduceti elementele: ";   cin >> val;
        a.add(val);
    }
    a.toString();

    cout << endl << "Elementul cautat este:";  cin>>val;
    if(a.contains(val))
        cout << "Acest element exista!";
    else
        cout << "Acest element nu exista!";

    cout << endl;
    cout << "Element de eliminat: ";    cin >> val;
    a.elimina(val);
    cout << endl;
    a.toString();



    return 0;
}
